    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "mongoose": "^5.4.3",
    "morgan": "^1.9.1"
  }
